<table class="table table-striped">
      <thead>
        <tr>
          <th>Title</th>
          <th>Type</th>
          <th>Genre</th>
          <th>Level</th>
          <th>Course</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @entries.each do |entry| %>
          <tr>
            <td><%= entry.title %></td>
            <td><%= entry.entry_type %></td>
            <td><%= entry.genre %></td>
            <td><% for @academic_level in entry.academic_levels %>
              <%= @academic_level.description %><% end %></td>
            <td><%= entry.course %></td>
            <td><%= link_to 'Show', entry %></td>
            <td><%= link_to 'Edit', edit_entry_path(entry) %></td>
            <td><%= link_to 'Destroy', entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>