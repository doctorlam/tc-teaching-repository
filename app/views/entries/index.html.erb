      <input type="text" class="search form-control" placeholder="Search Resources" /><br>
       <%= render 'filters' %>
    </div><!--/col-md-4-->
    
    <div class="col-md-8">
          <h1>Tech Comm Resources</h1>
         

          <div id="entries"><%= render 'entries' %></div>
