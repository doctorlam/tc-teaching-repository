</div><!--/container-->
</div><!--/test-list-->
<script type = "text/javascript">
$(function () {
  $('input[type=checkbox]').change(function(){
  $.get($('.entries_search').attr('action'),
  $('.entries_search').serialize(), null, 'script');
  return false;

});

  $('.entries_search').submit(function () {
    $.get(this.action, $(this).serialize(), null, 'script');
    return false;
  });
});
</script> 
<script type = "text/javascript">
var userList = new List('all-resources', { 
  valueNames: ['name', 'topic', 'type', 'level'],
   page: 10,
  pagination: true

});
</script>
